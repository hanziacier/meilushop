/* 广告图轮播样式*/

#banner {
	width: 100%;
	overflow: hidden;
	position: relative;
}

#banner .swiper-slide {
	position: relative;
	text-align: center;
}

#banner img {
	max-width: 100%;
}

.pagination {
	position: absolute;
	z-index: 20;
	bottom: 15px;
	width: 100%;
	text-align: center;
	right: 0;
}

.swiper-pagination-bullet {
	display: inline-block;
	width: 0.55rem;
	height: 0.55rem;
	border-radius: 50%;
	border: 1px solid rgb(255, 255, 255);
	margin: 0 2px;
	opacity: 1;
	background: none;
	cursor: pointer;
}

.swiper-pagination-bullet-active {
	background: #333;
	border: 1px solid #333;
}


