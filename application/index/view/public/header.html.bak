<!doctype html><html lang="zh-cn"><head>	<meta charset="utf-8">	<title>{present name="meta_title"}{$meta_title}_{/present}{:C('WEB_SITE_TITLE')}</title>	<link href="__CSS__/base.css" rel="stylesheet" type="text/css"/><link href="__CSS__/side.css" rel="stylesheet" type="text/css"/>	<link href="__CSS__/list.css" rel="stylesheet" type="text/css"/>	<link href="__CSS__/style.css" rel="stylesheet" type="text/css"/>	<script type="text/javascript" src="__COMMON__/jquery.min.js"></script>	<script type="text/javascript" src="__JS__/base.js"></script>	  {include file='public/color'}</head><body>	<!--header start-->	<div class="header w100">		<div class="header_in in_width f12">			{if condition="is_login()"}			<div class="header_lef fl">				<p>					您好 {$UcenterMember.username|default=''}									，欢迎来到					<a href="javascript:;" class="sc_a">yershop商城</a>					<a href="{:U('User/logout')}" class="tc_a">［退出］</a>				</p>			</div>{else/}<div class="header_lef fl header_lef_wdl">				<div class="wzc">					<span class="fl">嗨，欢迎光临yershop商城</span>										<a href="{:U('User/login')}" class="login_a fl">［登录］</a>					<a href="{:U('User/register')}" class="zc_a fl">［免费注册］</a>				</div>			</div>{/if}			<div class="header_rgt fr">								<dl class="">					<dt><a href="{:U('order/index')}">我的订单</a></dt>									</dl>				<dl class="">					<dt><a href="{:U('collect/index')}">我的收藏</a></dt>									</dl>				<dl class="">					<dt><a href="{:U('help/index?id=6')}">手机商城</a></dt>									</dl>			    <dl class="weixin">					<dt>关注我们<i></i></dt>					<dd>					    <h4>扫描二维码<br>						关注商城微信号</h4>					    <img src="__IMG__/code_pic.gif"> 					</dd>				</dl>											</div>		</div>		</div>	<!--/header end-->		<!--logo start-->	<div class="logo w100">		<div class="logo_in in_width">			<h1 class="logo_pic fl"><a href="{:U('index/index')}"><img title="YERSHOP" src="{:C('LOGO')}" width="201" height="59" /></a></h1>						<div class="head-search-layout">				<div class="head-search-bar" id="head-search-bar">					<div class="hd_serach_tab" id="hdSearchTab">						                    </div>                     <form class="search-form" id="top_search_form" action="{:url('search/index')}" method="post">						<input name="act" id="search_act" value="search" type="hidden">                                  <input name="keyword" id="keyword" type="text" class="input-text ui-autocomplete-input" value="" maxlength="60" x-webkit-speech="" lang="zh-CN" onwebkitspeechchange="foo()" placeholder="请输入您要搜索的商品的关键字" data-value="" x-webkit-grammar="builtin:search" autocomplete="off" role="textbox" aria-autocomplete="list" aria-haspopup="true">                        <input type="submit" id="button" value="搜索" class="input-submit">						<p>热门搜索:{foreach name="hotsearch" item="vo"}<a href="{:url('Search/index?keyword='.$vo)}">{$vo}</a>{/foreach}</p>                    </form>                   				</div>							</div>									</div>	</div>	<!--/logo end--> {if condition="$action neq 'cart/index'"}	<!--nav start-->	<div class="nav w100 nav_header">		<div class="nav_in in_width">			<div class="sidebar_div fl">				<div class="title">					<i></i>					<h2 class="f14" id="all-class">全部商品分类</h2>				</div>				<ul class="left_sidebar" id="all-goods">									{volist name="category" id="vo" offset="0" length="9"}					<li class="left_sidebar_li">						<div class="product">							<div class="border">																<div class="fl_div fl"><h3><a href="{:U('Goods/lists?id='.$vo['id'])}">{$vo.title}</a></h3>								</div>								<span class="fr pic arrow"></span>							</div>													</div>						<div class="product-wrap f14">							<div class="menu_div">																<div class="sub-class" >									<div class="sub-class-content">										<div class="recommend-class">																				</div>										  {volist name="vo['child']" id="vo1"}										<dl>											<dt><h3><a href="{:U('Goods/lists?id='.$vo1['id'])}">{$vo1.title}</a></h3></dt>											<dd class="goods-class">												{volist name="vo1['child']" id="vo2"}<a href="{:U('Goods/lists?id='.$vo2['id'])}">{$vo2.title}</a>											 {/volist}											</dd>										</dl> {/volist}									</div>								</div>															</div>						</div>					</li>{/volist}				</ul>			</div>						<ul class="nav_list fl">			  {volist name="channel" id="vo" key="k" empty=""}				<li class="{if condition="$current_url eq $vo['url']" }current{/if} fl"><a class="f16"href="{$vo.url|get_url}">{$vo.title}</a></li>				{/volist}			</ul>		</div>	</div>	<!--/nav end-->	{/if}	<script>var del_url="{:url('cart/delItem')}";var post_url="{:url('cart/addItem')}";	var dec_url="{:url('cart/decNum')}";var inc_url="{:url('cart/incNum')}";var img_path="__IMG__";var order_url="{:url('Order/add')}";var del_cart_url='{:url("cart/delCart")}';var clear_cart_url='{:url("cart/clear")}';var collect_url='{:url("collect/add")}';</script>